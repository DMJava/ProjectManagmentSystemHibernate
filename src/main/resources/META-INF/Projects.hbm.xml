<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ProjectManagmentSystem.pojo.Projects" table="projects">
        <id name="project_id" type="java.lang.Integer">
            <column name="project_id"/>
            <generator class="identity"/>
        </id>
        <property name="project_name" type="java.lang.String">
            <column name="project_name" length="45" not-null="true"/>
        </property>
        <property name="cost" type="java.lang.Integer">
            <column name="cost" length="11" not-null="true"/>
        </property>
        <set name="developerList" table="Projects_has_developers"
        inverse="false" lazy="true" fetch="select" cascade="all">
            <key>
                <column name="project_id" not-null="true"/>
            </key>
            <many-to-many entity-name="entity.Developers">
                <column name="developer_id" not-null="true"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>